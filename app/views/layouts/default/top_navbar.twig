<!DOCTYPE html>
<html lang="en">
<head>
{% block head %}
	{% block meta %}
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
	{% endblock %}

	<!-- Start Stylesheets -->
	{% block style %}
		{% include 'layouts.segments.style' %}
	{% endblock %}
	<!-- End Stylesheets -->


	<!-- Start Header Javascript -->
	{% block headscript %}
	{% endblock %}

	<!-- HTML5 Shiv -->
	<!--[if lt IE 9]>
		<script src="/assets/vendor/html5shiv/html5shiv.js"></script>
	<![endif]-->

	<!-- End Header Javascript -->

	<title>{% block title %}Larastrap Project Title{% endblock %}</title>
{% endblock %}
</head>
<body>
	<!-- Begin Navbar -->
	{% block navbar %}
	<div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
		<div class="container">
			<div class="navbar-header">
				<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
					<span class="sr-only">Toggle navigation</span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</button>
				<a class="navbar-brand" href="/">{% block navbar_brand %}Larastrap Project{% endblock %}</a>
			</div>
			<div class="navbar-collapse collapse">
				{% block navbar_left %}
					<!-- <ul class="nav navbar-nav">
						<li><a href="/">Home</a></li>
					</ul> -->
				{% endblock %}
				{% block navbar_right %}
                <!-- <ul class="nav navbar-nav navbar-right">
						<li><a href="/login">Login</a></li>
					</ul> -->
				{% endblock %}
			</div>
		</div>
	</div>
	{% endblock %}
	<!-- End Navbar -->

	<!-- Start Entire Page Wrap -->
	{% block body_wrap %}
	<div id="wrap">
		<!-- Begin Page Content -->
		<div class="container">
			{% block body_content %}{% endblock %}
		</div>
		<!-- End Page Content -->
	</div>
	{% endblock %}
	<!-- End Entire Page Wrap -->

	<!-- Start Footer -->
	{% block footer %}
	<div id="footer">
		<div class="container">
			{% block footer_content %}
				<div class="row">
					<div class="col-md-4 col-md-offset-4">
                        <H6>
                            <a href="https://github.com/memphisphp/larastrap">Powered by Larastrap Project</a>
                        </H6>
                    </div>
				</div>
			{% endblock %}
		</div>
	</div>
	{% endblock %}
	<!-- End Footer -->

	{% block footerscript %}
		{% include 'layouts.segments.js' %}
	{% endblock %}
</body>
</html>
