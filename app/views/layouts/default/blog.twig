	<!-- Start Footer -->
	{% block footer %}
	<footer>
		<div class="container">
			{% block footer_content %}
			<div class="row">
				<div class="col-sm-6">
					<ul class="list-inline">
						<li><i class="fa fa-2x fa-facebook"></i></li>
						<li><i class="fa fa-2x fa-twitter"></i></li>
