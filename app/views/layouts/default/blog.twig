	<!-- Begin Blog Footer -->
	{% block blog_footer %}
	<div class="container" id="blog-footer">
		{% block blog_footer_content %}
		<div class="row">
			<div class="col col-sm-12">
				<h1>Follow Us</h1>
				<div class="btn-group">
					<a class="btn btn-twitter btn-lg" href="#"><i class="fa fa-twitter"></i> Twitter</a>
					<a class="btn btn-facebook btn-lg" href="#"><i class="fa fa-facebook"></i> Facebook</a>
